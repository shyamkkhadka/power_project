
<%= form_for([@battery, @battery_daily_test]) do |f| %>
  <% @banks.each do |bank| %>
		Bank ID : <%= bank.id %>
     <%= fields_for "banks[#{bank.id}]", bank do |g| %>
    	<p>Total Voltage <%= g.text_field :total_voltage %></p>
    	<p>Visual Inspection <%= g.text_field :visual_inspection %></p>
    	<%= g.hidden_field :bank_id, :value => bank.id %></p>
  <% end %>
  <% end %>
		<p> <%= f.submit %> </p>
<% end %>

